{
	"name": "blac-studios",
	"version": "1.0.0",
	"description": "A Portfolio based project",
	"main": "js/app.js",
	"scripts": {
		"watch:sass": "node-sass sass/main.scss css/style.css -w",
		"start": "npm-run-all watch:sass",
		"compile:sass": "node-sass sass/main.scss css/style.comp.css",
		"concat:css": "concat -o css/style.concat.css fonts/icomoon.css fonts/ionicons.min.css css/style.comp.css",
		"compress:css": "node-sass css/style.concat.css css/style.css --output-style compressed",
		"build:css": "npm-run-all compile:sass concat:css compress:css"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Kevin-kirago/Blac-Studios.git"
	},
	"author": "Kevin Kiragu",
	"license": "ISC",
	"devDependencies": {
		"concat": "^1.0.3",
		"node-sass": "^4.12.0",
		"npm-run-all": "^4.1.5"
	},
	"dependencies": {
		"aos": "^2.3.4",
		"bootstrap": "^4.3.1",
		"isotope-layout": "^3.0.6",
		"jquery": "^3.4.1",
		"popper.js": "^1.15.0"
	}
}
